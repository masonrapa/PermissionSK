			set {permission.%arg 1%} to true
		if {rango} is "pandito":
			set {pandito.%arg 1%} to true
			set {admin.%arg 1%} to false
			set {staff.%arg 1%} to false
			set {owner.%arg 1%} to false
			set {rage.%arg 1%} to false
			set {vip.%arg 1%} to false
